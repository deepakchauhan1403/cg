Code:
#include <stdio.h>
#include <conio.h>
#include <graphics.h>
int main()
{
    int gd, gm;
    char path[] = "C:\\MinGW\\lib\\libbgi.a";
    detectgraph(&gd, &gm);
    initgraph(&gd, &gm, path);
    circle(300, 200, 150);       // Face.
    circle(235, 160, 35);        // Eye1.
    circle(360, 160, 35);        // Eye2.
    arc(300, 230, 180, 360, 75); // Smile
    getch();
    closegraph();
    return 0;
}

 
